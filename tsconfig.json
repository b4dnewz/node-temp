{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "rootDir": "src",
    "outDir": "lib",
    "declaration": true,
    "esModuleInterop": true
  },
  "include": [
    "src/**/*.ts"
  ],
  "typedocOptions": {
    "readme": "none",
    "mode": "file",
    "out": "docs/.vuepress/public/api",
    "excludePrivate": true
  }
}
